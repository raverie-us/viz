{
  "compilerOptions": {
    "sourceMap": true,
    "strict": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "target": "ES6",
    "removeComments": false,
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "esModuleInterop": true,
    "baseUrl": "./",
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "types": [
      "node",
      "resize-observer-browser"
    ]
  },
  "exclude": [
      "node_modules",
      "dist",
      "docker"
  ],
  "files": ["./frontend/index.tsx"]
}